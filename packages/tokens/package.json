{
  "name": "@avnir/tokens",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "files": ["dist"],
  "exports": {
    "./themes.css": "./dist/themes.css",
    "./tailwind-preset": "./dist/tailwind-preset.cjs"
  },
  "scripts": {
    "build": "pnpm run build:css && pnpm run build:preset",
    "build:css": "mkdir -p dist && cp src/themes.css dist/themes.css",
    "build:preset": "mkdir -p dist && cp src/tailwind-preset.cjs dist/tailwind-preset.cjs",
    "lint": "echo 'No lint configured'",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .turbo dist"
  },
  "devDependencies": {
    "rimraf": "^6.0.0"
  }
}
